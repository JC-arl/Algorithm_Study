#include <iostream>
#include <stack>
#include <queue>
#include <string>
using namespace std;
int N,n,count=0;

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    
    stack<int> st;
    queue<char> qu;
    cin>>N;
    
    cin>> n;
    for(int i=1; i<=N; i++)
    {
        st.push(i);
        qu.push('+');
        while(true)
        {
            if(st.empty())
                break;
            if(st.top() == n)
            {
                st.pop();
                qu.push('-');
                if(count == N -1)   break;
                
                cin>> n;
                count++;
            }
            else break;
        }
        
    }
    if(st.empty())
    {
        while(!qu.empty())
        {
            cout<<qu.front()<<"\n";
            qu.pop();
        }    
    }
    else cout<<"NO";


    return 0;
}
